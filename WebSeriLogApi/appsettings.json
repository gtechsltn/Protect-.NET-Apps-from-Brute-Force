{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "IpRateLimiting": {
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1s",
        "Limit": 2
      },
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 100
      }
    ]
  },
  "IpRateLimitingPolicies": {
    "ClientId": {
      "EnableRateLimiting": true,
      "GeneralRules": [
        {
          "Endpoint": "*",
          "Period": "1h",
          "Limit": 1000
        }
      ]
    }
  },
  "AllowedHosts": "*"
}
